<?xml version="1.0" encoding="UTF-8"?>

<container xmlns="http://symfony.com/schema/dic/services"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">

    <services>
        <defaults public="true" />

        <service id="setono_sylius_bulk_discount.behat.context.cli.commands"
                 class="Tests\Setono\SyliusBulkDiscountPlugin\Behat\Context\Cli\CommandsContext">
            <argument type="service" id="sylius.behat.shared_storage" />
            <argument type="service" id="kernel" />
            <argument type="service" id="setono_sylius_bulk_discount.command.check_active" />
            <argument type="service" id="setono_sylius_bulk_discount.command.assign_specials" />
            <argument type="service" id="setono_sylius_bulk_discount.command.product_recalculate" />
            <argument type="service" id="setono_sylius_bulk_discount.command.special_recalculate" />
        </service>
    </services>
</container>
